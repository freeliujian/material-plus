{
  "name": "material-plus",
  "version": "1.0.0",
  "description": "storybook",
  "keyword": "rollup react storybook",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "scripts": {
    "start": "npm-run-all -p start:sb",
    "build": "rimraf build/* && rollup -c",
    "start:sb": "start-storybook -p 6006",
    "build:sb": "build-storybook -c .storybook -o docs",
    "start:ls": "npm run build-css:ls && run-p -ncr watch-css:ls",
    "build:ls": "run-s -n build-css:ls",
    "build-css:ls": "node-less-chokidar stories",
    "watch-css:ls": "node-less-chokidar stories --watch",
    "test": "run-s -n build-css test-js"
  },
  "author": "jian.liu",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "classnames": "^2.3.1",
    "macy": "^2.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "0.0.11",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.14.0",
    "less": "^4.1.2",
    "less-loader": "^5.0.0",
    "node-less-chokidar": "^0.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "rollup": "^2.70.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "style-loader": "^1.3.0",
    "ts-loader": "^9.2.8",
    "tslib": "^2.4.0"
  }
}
